{
  "name": "@macalinao/progenitor",
  "version": "0.1.0",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "progenitor": "./dist/bin/progenitor.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "lint": "bunx eslint .",
    "test": "bun test || true",
