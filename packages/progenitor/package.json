    "dev": "stricli-kit dev"
  },
  "dependencies": {
    "@macalinao/stricli-kit": "workspace:^",
    "@stricli/core": "catalog:"
  },
  "devDependencies": {
    "@macalinao/tsconfig": "catalog:",
    "@types/bun": "catalog:",
    "@types/node": "^22.0.0",
    "typescript": "catalog:"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
